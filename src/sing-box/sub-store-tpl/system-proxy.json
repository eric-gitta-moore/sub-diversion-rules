{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui_download_url": "https://github.com/MetaCubeX/Yacd-meta/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "默认"
    }
  },
  "dns": {
    "strategy": "ipv4_only",
    "servers": [
      {
        "tag": "remote",
        "address": "tcp://8.8.8.8",
        "detour": "默认"
      },
      {
        "tag": "local",
        "address": "223.5.5.5",
        "detour": "DIRECT"
      }
    ],
    "rules": [
      {
        "server": "local",
        "rule_set": [
          "GeoSite@private",
          "MainlandNetwork",
          "GeoSite@cn"
        ]
      },
      {
        "server": "local",
        "outbound": [
          "any"
        ]
      }
    ]
  },
  "route": {
    "final": "默认",
    "auto_detect_interface": true,
    "rule_set": [
      {
        "tag": "MainlandNetwork",
        "download_detour": "DIRECT",
        "type": "remote",
        "format": "source",
        "url": "https://gcore.jsdelivr.net/gh/eric-gitta-moore/sub-diversion-rules@main/rule/james-curtis/Clash/Providers/MainlandNetwork.json"
      },
      {
        "tag": "GoogleOneVPNTun",
        "download_detour": "DIRECT",
        "type": "remote",
        "format": "source",
        "url": "https://gcore.jsdelivr.net/gh/eric-gitta-moore/sub-diversion-rules@main/rule/james-curtis/Clash/Providers/GoogleOneVPNTun.json"
      },
      {
        "tag": "GoogleOneVPNCheckPoint",
        "download_detour": "DIRECT",
        "type": "remote",
        "format": "source",
        "url": "https://gcore.jsdelivr.net/gh/eric-gitta-moore/sub-diversion-rules@main/rule/james-curtis/Clash/Providers/GoogleOneVPNCheckPoint.json"
      },
      {
        "tag": "GeoSite@cn",
        "download_detour": "DIRECT",
        "type": "remote",
        "format": "binary",
        "url": "https://gcore.jsdelivr.net/gh/eric-gitta-moore/meta-rules-dat@sing/geo/geosite/cn.srs"
      },
      {
        "tag": "GeoSite@private",
        "download_detour": "DIRECT",
        "type": "remote",
        "format": "binary",
        "url": "https://gcore.jsdelivr.net/gh/eric-gitta-moore/meta-rules-dat@sing/geo/geosite/private.srs"
      },
      {
        "tag": "GeoIP@cn",
        "download_detour": "DIRECT",
        "type": "remote",
        "format": "binary",
        "url": "https://gcore.jsdelivr.net/gh/eric-gitta-moore/meta-rules-dat@sing/geo/geoip/cn.srs"
      },
      {
        "tag": "GeoIP@private",
        "download_detour": "DIRECT",
        "type": "remote",
        "format": "binary",
        "url": "https://gcore.jsdelivr.net/gh/eric-gitta-moore/meta-rules-dat@sing/geo/geoip/private.srs"
      }
    ],
    "rules": [
      {
        "outbound": "dns-out",
        "protocol": "dns"
      },
      {
        "outbound": "GoogleOneVPNCheckPoint",
        "rule_set": [
          "GoogleOneVPNCheckPoint"
        ]
      },
      {
        "outbound": "GoogleOneVPNTun",
        "rule_set": [
          "GoogleOneVPNTun"
        ]
      },
      {
        "outbound": "国内",
        "rule_set": [
          "GeoSite@private",
          "MainlandNetwork",
          "GeoSite@cn"
        ]
      },
      {
        "outbound": "国内",
        "rule_set": [
          "GeoIP@private",
          "GeoIP@cn"
        ]
      }
    ]
  },
  "outbounds": [
    {
      "tag": "默认",
      "type": "selector",
      "outbounds": [
        "自动选择",
        "手动选择",
        "DIRECT"
      ]
    },
    {
      "tag": "自动选择",
      "type": "urltest",
      "url": "http://cp.cloudflare.com/",
      "interval": "4h",
      "idle_timeout": "8h"
    },
    {
      "tag": "手动选择",
      "type": "selector"
    },
    {
      "tag": "前置代理组",
      "type": "selector"
    },
    {
      "tag": "GoogleOneVPNCheckPoint",
      "outbounds": [
        "默认",
        "自动选择",
        "手动选择",
        "DIRECT"
      ],
      "type": "selector"
    },
    {
      "tag": "GoogleOneVPNTun",
      "outbounds": [
        "默认",
        "自动选择",
        "手动选择",
        "DIRECT"
      ],
      "type": "selector"
    },
    {
      "tag": "中转走落地",
      "type": "selector"
    },
    {
      "tag": "落地代理",
      "type": "selector"
    },
    {
      "tag": "国内",
      "outbounds": [
        "DIRECT",
        "默认",
        "自动选择",
        "手动选择"
      ],
      "type": "selector"
    },
    {
      "tag": "DIRECT",
      "type": "direct"
    },
    {
      "tag": "dns-out",
      "type": "dns"
    }
  ],
  "inbounds": [
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "127.0.0.1",
      "listen_port": 2080,
      "sniff": true,
      "sniff_override_destination": true,
      "domain_strategy": "prefer_ipv4"
    }
  ]
}